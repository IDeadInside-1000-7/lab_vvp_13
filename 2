#include <stdio.h>
#include <locale.h>
int main()
{
	setlocale(LC_ALL, "Russian");
	int N, i;
	printf("Введите целое число N (> 0): ");
	scanf("%i", &N);
	printf("\nПрограмма выведет произведение 1.1 · 1.2 · 1.3 · ...(N сомножителей)\n\n");
	float c = 1.1;
	float s = 1;
	for (i = 0; i < N; i += 1)
	{

		s *= c;
		c += 0.1;

	}
	printf("Результат: %f", s);
}
